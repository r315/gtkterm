msgid "--bits <bits> or -b : number of bits (default 8)\n"
msgstr "--bits <бит> или -b : число бит данных (по умолчанию 8)\n"

msgid ""
"--char <char> or -r : wait for a special char at end of line (default none)\n"
msgstr ""
"--char <символ> или -r : ожидать специальный символ окончания строки (по "
"умолчанию выкл)\n"

msgid "--config <configuration> or -c : load configuration\n"
msgstr "--config <файл_настроек> или -c : использовать настройки из файла\n"

msgid "--delay <ms> or -d : end of line delay in ms (default none)\n"
msgstr ""
"--delay <мс> или -d : задержка в мс после каждой строки (по умолчанию выкл)\n"

msgid "--echo or -e : switch on local echo\n"
msgstr "--echo или -e : отображать введенные символы на экране\n"

msgid "--file <filename> or -f : default file to send (default none)\n"
msgstr ""
"--file <имя_файла> или -f : имя файла для отправки (по умолчанию нет)\n"

msgid "--flow <Xon | RTS | RS485> or -w : flow control (default none)\n"
msgstr ""
"--flow <Xon | RTS | RS485> или -w : управление потоком (по умолчанию выкл)\n"

msgid "--help or -h : this help screen\n"
msgstr "--help или -h : показать эту справку\n"

msgid "--parity <odd | even> or -a : partity (default none)\n"
msgstr "--parity <odd | even> или -a : четность (по умолчанию выкл)\n"

msgid "--port <device> or -p : serial port device (default /dev/ttyS0)\n"
msgstr ""
"--port <устройство> или -p : последовательный порт (по умолчанию /dev/"
"ttyS0)\n"

msgid ""
"--rts_time_after <ms> or -y : for rs485, time in ms after transmit with rts "
"on\n"
msgstr ""
"--rts_time_after <ms> или -y : для rs485, время в ms после передачи с rts "
"on\n"

msgid ""
"--rts_time_before <ms> or -x : for rs485, time in ms before transmit with "
"rts on\n"
msgstr ""
"--rts_time_before <ms> или -x : для rs485, время в ms перед установкой rts "
"on\n"

msgid "--speed <speed> or -s : serial port speed (default 9600)\n"
msgstr "--speed <скорость> или -s : скорость порта (по умолчанию 9600)\n"

msgid "--stopbits <stopbits> or -t : number of stopbits (default 1)\n"
msgstr "--stopbits <стоп_бит> или -t : число стоповых бит (по умолчанию 1)\n"

